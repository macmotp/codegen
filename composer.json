{
  "name": "macmotp/codegen",
  "version": "v0.2.0",
  "description": "Generate human friendly codes",
  "keywords": [
    "macmotp",
    "codegen",
    "code",
    "code generator",
    "human friendly code",
    "reference code",
    "semantic code",
    "receipt numbers"
  ],
  "homepage": "https://github.com/macmotp/codegen",
  "license": "MIT",
  "authors": [
    {
      "name": "Marco Gava",
      "email": "marco.gava@me.com",
      "homepage": "https://github.com/macmotp",
      "role": "Software Engineer"
    }
  ],
  "require": {
    "php": ">=8.0",
    "ext-intl": "*"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.21",
    "phpunit/phpunit": "^10.2"
  },
  "autoload": {
    "psr-4": {
      "Macmotp\\": "src/Codegen"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Macmotp\\Codegen\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit --no-coverage",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
    "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
